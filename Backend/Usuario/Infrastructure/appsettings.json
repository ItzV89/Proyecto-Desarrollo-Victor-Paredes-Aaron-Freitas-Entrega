{
	"ReverseProxy": {
		"Routes": {
			"ruta-perfil-me": {
				"ClusterId": "cluster-usuarios",
				"Match": { "Path": "/api/perfil/me" },
				"AuthorizationPolicy": "UsuarioAutenticado" 
			},
			"ruta-admin-usuarios": {
				"ClusterId": "cluster-usuarios",
				"Match": { "Path": "/api/admin/usuarios" },
				"AuthorizationPolicy": "SoloAdmin" 
			}
		},
		"Clusters": {
			"cluster-usuarios": {
				"Destinations": {
					"destino1": {
						
						"Address": "http://localhost:5001"
					}
				}
			}
		}
	}
}